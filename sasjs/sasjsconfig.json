{
  "$schema": "https://cli.sasjs.io/sasjsconfig-schema.json",
  "macroFolders": ["sasjs/macros"],
  "programFolders": [],
  "serviceConfig": {
    "serviceFolders": ["sasjs/services/common"],
    "initProgram": "",
    "termProgram": "",
    "macroVars": {}
  },
  "streamConfig": {
    "assetPaths": [""],
    "streamWeb": false,
    "streamWebFolder": "webv",
    "webSourcePath": "build"
  },
  "defaultTarget": "viya",
  "targets": [
    {
      "name": "viya",
      "serverUrl": "https://sas.analytium.co.uk",
      "serverType": "SASVIYA",
      "appLoc": "/Public/app/react-seed-app",
      "deployConfig": {
        "deployServicePack": true
      }
    },
    {
      "name": "sas9",
      "serverType": "SAS9",
      "appLoc": "/User Folders/&sysuserid/My Folder/react-seed-app",
      "deployConfig": {
        "deployServicePack": true
      },
      "serverName": "SASApp",
      "repositoryName": "Foundation"
    }
  ]
}
